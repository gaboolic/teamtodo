<request type="im" subtype="notify" msid="">
  <token>${token!}</token>
  <message sync="${sync!}" >
    <im>
      <title>${title!}</title>
      <subject>${subject!}</subject>
      <content>${content!}</content>
      <receiver>${receiver!}</receiver>
      <url>${url!}</url>
    </im>
  </message>
</request>